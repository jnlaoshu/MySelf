name: TikTok解锁-俄罗斯
desc: 解锁俄罗斯地区限制

http:
  mitm:
    - "*.tiktokv.com"
    - "*.byteoversea.com"
    - "*.tik-tokapi.com"
  rewrite:
    # 第一行RU为观看的地区，可自行修改为其它地区
    - (?<=_region=)CN(?=&) RU 307
    - (?<=&mcc_mnc=)4 2 307
    - ^(https?:\/\/(tnc|dm)[\w-]+\.\w+\.com\/.+)(\?)(.+) $1$3 302
    - (^https?:\/\/*\.\w{4}okv.com\/.+&.+)(\d{2}\.3\.\d)(.+) $118.0$3 302
